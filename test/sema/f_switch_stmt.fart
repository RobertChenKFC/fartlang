class Main {
  f() {
    switch 3 {
      case 5 { # correct
        var x = 10;
        x = 5; # correct
      }
      case 3.5 { # correct
        x = 5; # error
      }
      case null { # error
        var x = "hello"; # correct
      }
      default {
        var x = true; # correct
        switch true {
          case x == x, x != x {} # correct
          case 3 {} # error
          case true, 5 {} # error
        }
      }
    }
  }
}
